    "postcss-preset-env": "^10.1.2",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.4.2",
    "prettier-eslint": "^16.3.0",
    "prettier-eslint-cli": "^8.0.1",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "stylelint": "^16.12.0",
    "stylelint-config-standard": "^36.0.1",
    "tailwindcss": "^3.4.17",
    "tailwindcss-font-size": "^1.0.0"
